{
  "name": "@yulesxoxo/svg-to-svelte",
  "version": "0.0.1",
  "description": "Convert SVG files into Svelte 5 components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "svg-to-svelte": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "svelte5",
    "svg",
    "svelte-component",
    "svg-to-svelte",
    "cli",
    "command-line",
    "icons",
    "components",
    "svg-converter"
  ],
  "author": "yulesxoxo (https://github.com/yulesxoxo)",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "prettier-write": "prettier --write \"**/*.{ts,js}\""
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.0",
    "svgo": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yulesxoxo/svg-to-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/yulesxoxo/svg-to-svelte/issues"
  },
  "homepage": "https://github.com/yulesxoxo/svg-to-svelte#readme"
}
